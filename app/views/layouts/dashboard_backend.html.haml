!!! 5
%html.no-js{ data: { google_analytics_id: ::Settings.google_analytics_id } }
  %head
    = render 'layouts/head'
    = csrf_meta_tags
  %body{ class: body_class }
    #wrap
      = render 'layouts/navigation', navbar_style: 'navbar-inverse'
      .container-fluid
        .row
          %aside.hidden-xs.hidden-sm.col-md-2.sidenav{ role: 'aside' }
            = yield(:sidenav)
          %main.col-sm-12.col-md-10.col-lg-9.col-xl-8.main{ role: 'main' }
            = render 'layouts/messages'
            = yield
    = render 'layouts/footer'
= render_summary
