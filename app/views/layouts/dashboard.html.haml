!!! 5
%html
  %head
    = render 'layouts/head'
    = csrf_meta_tags
  %body{ class: body_class }
    #wrap
      = render 'layouts/navigation'
      .container-fluid
        .row
          %aside.col-md-2.sidenav(role='aside')
            = render 'sidenav'
          %main.col-md-10.main(role='main')
            = render 'layouts/messages'
            = yield
    = render 'layouts/footer'